ttpKernel\Debug\StopwatchEvent␀periods";a:1:{i:0;a:2:{i:0;d:580.60000000000002;i:1;d:582.5;}}s:57:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀origin";d:1363803097924.5;s:59:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀category";s:7:"section";s:58:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀started";a:0:{}}s:19:"EmailSenderListener";O:49:"Symfony\Component\HttpKernel\Debug\StopwatchEvent":4:{s:58:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀periods";a:1:{i:0;a:2:{i:0;d:582.39999999999998;i:1;d:582.5;}}s:57:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀origin";d:1363803097924.5;s:59:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀category";s:14:"event_listener";s:58:"␀Symfony\Component\HttpKernel\Debug\StopwatchEvent␀started";a:0:{}}}}}s:6:"memory";C:62:"Symfony\Component\HttpKernel\DataCollector\MemoryDataCollector":30:{a:1:{s:6:"memory";i:52690944;}}s:6:"router";C:64:"Symfony\Bundle\FrameworkBundle\DataCollector\RouterDataCollector":129:{a:3:{s:8:"redirect";b:1;s:3:"url";s:64:"/sites/blog/web/app_dev.php/admin/tperroin/blogsio/projet/3/edit";s:5:"route";s:3:"n/a";}}s:8:"security";C:65:"Symfony\Bundle\SecurityBundle\DataCollector\SecurityDataCollector":138:{a:4:{s:7:"enabled";b:1;s:13:"authenticated";b:1;s:4:"user";s:5:"admin";s:5:"roles";a:2:{i:0;s:16:"ROLE_SUPER_ADMIN";i:1;s:9:"ROLE_USER";}}}s:11:"swiftmailer";C:61:"Symfony\Bridge\Swiftmailer\DataCollector\MessageDataCollector":69:{a:3:{s:8:"messages";a:0:{}s:12:"messageCount";i:0;s:7:"isSpool";b:1;}}s:2:"db";C:66:"Doctrine\Bundle\DoctrineBundle\DataCollector\DoctrineDataCollector":14993:{a:5:{s:7:"queries";a:1:{s:7:"default";a:5:{i:1;a:5:{s:3:"sql";s:1261:"SELECT t0.username AS username1, t0.username_canonical AS username_canonical2, t0.email AS email3, t0.email_canonical AS email_canonical4, t0.enabled AS enabled5, t0.salt AS salt6, t0.password AS password7, t0.last_login AS last_login8, t0.locked AS locked9, t0.expired AS expired10, t0.expires_at AS expires_at11, t0.confir